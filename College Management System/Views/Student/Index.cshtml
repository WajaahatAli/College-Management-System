@model IEnumerable<College_Management_System.Models.Attributes>

@{
    ViewData["Title"] = "Students List";
}

<h2>  Students </h2>

<p>

    <a asp-controller="Student" asp-action="Index" class="btn btn-primary">View students </a>

</p>.

<p> <a asp-controller="Student" asp-action="Createe" class="btn btn-success mb-3"> Create Students </a></p>




<table>
    class = "table table-bordered table - striped"
    <thead>
        <tr>


            <th>First Name</th>
            <th>Last Name </th>
            <th>Age</th>
            <th> Date of Birth</th>
            <th>Gender</th>
            <th> School</th>

        </tr>
    </thead>

    <tbody>
        @foreach (var students in Model)

        {
            <tr>
                <td>@students.FirstName</td>
                <td>@students.LastName</td>
                <td>@students.age</td>





            </tr>
        }




    </tbody>
</table>


